{
  "name": "ZmianyDograneXd",
  "description": "Ustawia treść komentarza w zależności od wybranej wersji naprawy ~ KubaBubaa",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://jira.coig.com/browse/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}